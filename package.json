{
  "name": "waifuu",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "next start",
    "prisma": "prisma",
    "nc": "new-component",
    "build:production": "cross-env NODE_ENV=production prisma migrate deploy && prisma db seed && npx lingui extract && npx lingui compile && next build",
    "build:staging": "cross-env NODE_ENV=production prisma migrate deploy && prisma db seed && npx lingui extract && npx lingui compile && next build",
    "build:development": "cross-env NODE_ENV=development next build",
    "build:native:production": "cross-env NODE_ENV=production NEXT_PUBLIC_BUILDING_NATIVE=1 next build",
    "build:native:development": "cross-env NODE_ENV=development NEXT_PUBLIC_BUILDING_NATIVE=1 next build",
    "dev": "cross-env NODE_ENV=development NEXT_PUBLIC_DEVELOPMENT=1 next dev",
    "dev:external": "cross-env HOST=0.0.0.0 && pnpm dev",
    "lint": "biome lint --apply-unsafe ./src",
    "lint:check": "biome lint ./src",
    "format": "biome format --write ./src",
    "fix": "biome check --apply-unsafe ./src",
    "env:pull": "dotenv-vault pull -y",
    "env:push": "dotenv-vault push -y",
    "env:open": "dotenv-vault open -y",
    "env:build": "dotenv-vault build",
    "env:keys": "dotenv-vault keys",
    "t:extract": "lingui extract",
    "t:compile": "lingui compile",
    "t:push": "crowdin push sources && crowdin push translations",
    "t:push:translate": "crowdin push sources && crowdin push translations && pnpm t:pre-translate",
    "t:pull": "crowdin pull && pnpm t:compile",
    "t:pre-translate": "crowdin pre-translate --method=mt --engine-id=410300",
    "t:sync": "pnpm t:extract && pnpm t:push && pnpm t:pull",
    "t:sync:translate": "pnpm t:extract && pnpm t:push && pnpm t:pre-translate && pnpm t:pull",
    "prisma:reset": "prisma db push --force-reset && prisma db seed",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/stripe_webhooks --project-name=companion"
  },
  "prisma": {
    "seed": "pnpm dlx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "biome check --no-errors-on-unmatched --apply-unsafe --organize-imports-enabled true"
    ]
  },
